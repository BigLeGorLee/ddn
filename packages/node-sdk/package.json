{
  "name": "@ddn/node-sdk",
  "version": "0.3.8",
  "description": "DDN Node.js SDK",
  "main": "index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "test": "mocha test",
    "build": "yarn build:dev & yarn build:prod",
    "build:dev": "browserify index.browser.js > ./dist/ddn.browserify.js",
    "build:prod": "browserify index.browser.js -p tinyify > ./dist/ddn.browserify.min.js"
  },
  "keywords": [
    "ddn",
    "node",
    "sdk"
  ],
  "author": "wxx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/ddnlink/ddn/tree/master/packages/node-sdk"
  },
  "homepage": "http://github.com/ddnlink/ddn/tree/master/packages/node-sdk",
  "bugs": {
    "url": "http://github.com/ddnlink/ddn/issues"
  },
  "dependencies": {
    "@ddn/asset-aob": "^0.2.5",
    "@ddn/asset-base": "^0.2.9",
    "@ddn/asset-dao": "^0.2.11",
    "@ddn/asset-dapp": "^0.2.11",
    "@ddn/asset-evidence": "^0.1.10",
    "@ddn/crypto": "^0.2.11",
    "@ddn/utils": "^0.1.10",
    "JSONStream": "^1.3.1",
    "buffer": "^4.7.0",
    "bytebuffer": "^5.0.1",
    "long": "^4.0.0",
    "ripemd160": "^2.0.1",
    "through2": "^2.0.3",
    "tweetnacl": "^1.0.2"
  },
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "devDependencies": {
    "browserify": "^13.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "tinyify": "^2.5.2"
  },
  "gitHead": "26a701eeebd9d9f8c72a73cb18080170f6fd4fde"
}
